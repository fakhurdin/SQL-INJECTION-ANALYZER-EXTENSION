{
    "manifest_version": 3,
    "name": "SQL Injection Scanner",
    "version": "1.0",
    "description": "An extension to scan websites for SQL injection vulnerabilities.",
    "permissions": ["scripting", "activeTab"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/1.png",
            "48": "icons/1.png",
            "128": "icons/1.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/1.png",
        "48": "icons/1.png",
        "128": "icons/1.png"
    }
}
